{
  "name": "KiloAssets",
  "version": "1.0.0",
  "description": "Assets for kilonova",
  "scripts": {
    "buildCSS": "TAILWIND_MODE=build postcss tailwind.css -o ../static/styles.css",
    "watchCSS": "TAILWIND_MODE=watch postcss tailwind.css -o ../static/styles.css --watch",
    "prodCSS": "NODE_ENV=production yarn buildCSS",
    "buildJS": "esbuild app.js --bundle --target=es2017 --outfile=../static/bundle.js --global-name=bundled --jsx-factory=h --jsx-fragment=Fragment --sourcemap",
    "watchJS": "esbuild app.js --bundle --target=es2017 --outfile=../static/bundle.js --global-name=bundled --jsx-factory=h --jsx-fragment=Fragment --sourcemap --watch",
    "prodJS": "esbuild app.js --bundle --target=es2017 --outfile=../static/bundle.js --global-name=bundled --jsx-factory=h --jsx-fragment=Fragment --minify --analyze",
    "prod": "yarn prodJS && yarn prodCSS",
    "build": "yarn buildJS && yarn buildCSS",
    "esv": "esbuild --version"
  },
  "license": "",
  "private": true,
  "dependencies": {
    "@codemirror/commands": "^0.19.6",
    "@codemirror/history": "^0.19.0",
    "@codemirror/state": "^0.19.6",
    "@codemirror/view": "^0.19.29",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.0",
    "autoprefixer": "^10.4.0",
    "cssnano": "^5.0.12",
    "dayjs": "^1.10.7",
    "esbuild": "^0.14.2",
    "fast-glob": "^3.2.7",
    "glob": "^7.2.0",
    "js-base64": "^3.7.2",
    "js-cookie": "^3.0.1",
    "notyf": "^3.10.0",
    "postcss": "^8.4.4",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^14.0.2",
    "preact": "^10.6.4",
    "preact-custom-element": "^4.2.1",
    "query-string": "^7.0.1",
    "sprintf-js": "^1.1.2",
    "tailwindcss": "^3.0.1",
    "toastr": "^2.1.4",
    "underscore": "^1.13.1"
  }
}
