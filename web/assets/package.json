{
	"name": "kilossets",
	"version": "1.0.0",
	"description": "Asset 'pipeline' for Kilonova",
	"scripts": {
		"watchCSS": "echo 'WARNING: All fonts WILL break.' && tailwindcss --postcss ./postcss.config.js -i tailwind.css --watch -o ../static/styles.css",
		"buildCSS": "TAILWIND_MODE=build postcss tailwind.css -o ../static/styles.css",
		"prodCSS": "NODE_ENV=production yarn buildCSS",
		"buildJS": "esbuild app.ts --jsx-factory=h --jsx-fragment=Fragment --bundle --target=es2015 --outfile=../static/bundle.js --global-name=bundled --sourcemap=inline ",
		"prodJS": "esbuild app.ts  --jsx-factory=h --jsx-fragment=Fragment --bundle --target=es2015 --outfile=../static/bundle.js --global-name=bundled --sourcemap --minify ",
		"vendor": "esbuild vendored.ts --bundle --target=es2015 --outfile=../static/vendored.js --global-name=vendored --sourcemap --minify",
		"prod": "yarn prodJS && yarn prodCSS",
		"build": "yarn buildJS && yarn buildCSS",
		"esv": "esbuild --version"
	},
	"license": "",
	"private": true,
	"dependencies": {
		"@fontsource/noto-sans": "^4.5.11",
		"@fortawesome/fontawesome-free": "^6.2.1",
		"@fullhuman/postcss-purgecss": "^5.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@tailwindcss/typography": "^0.5.8",
		"@types/codemirror": "^5.60.7",
		"@types/js-cookie": "^3.0.2",
		"@types/katex": "^0.16.0",
		"@types/sprintf-js": "^1.1.2",
		"@types/underscore": "^1.11.4",
		"autoprefixer": "^10.4.13",
		"codemirror": "^5.65.11",
		"cssnano": "^5.1.14",
		"dayjs": "^1.11.6",
		"esbuild": "^0.15.16",
		"glob": "^8.0.3",
		"highlight.js": "^11.7.0",
		"highlightjs-copy": "github:arronhunt/highlightjs-copy#339a65b6defd1a65f6bcc07c314938e3d32c3000",
		"js-base64": "^3.7.3",
		"js-cookie": "^3.0.1",
		"katex": "^0.16.4",
		"notyf": "^3.10.0",
		"postcss": "^8.4.19",
		"postcss-cli": "^10.1.0",
		"postcss-import": "^15.0.1",
		"postcss-url": "^10.1.3",
		"preact": "^10.11.3",
		"preact-custom-element": "^4.2.1",
		"query-string": "^7.1.2",
		"sprintf-js": "^1.1.2",
		"tailwindcss": "^3.2.4",
		"toastr": "^2.1.4",
		"underscore": "^1.13.6"
	}
}
