{
  "name": "Kilossets",
  "version": "1.0.0",
  "description": "Asset 'pipeline' for Kilonova",
  "scripts": {
    "buildCSS": "postcss tailwind.css -o ../static/styles.css",
    "prodCSS": "NODE_ENV=production yarn buildCSS",
    "buildJS": "esbuild app.js --bundle --outfile=../static/bundle.js --global-name=bundled",
    "prodJS": "esbuild app.js --bundle --minify --define:process.env.NODE_ENV=\\\"production\\\" --outfile=../static/bundle.js --global-name=bundled",
    "prod": "yarn prodJS && yarn prodCSS",
    "build": "yarn buildJS && yarn buildCSS",
    "esv": "esbuild --version"
  },
  "license": "",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^3.1.3",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.2.0",
    "cssnano": "^4.1.10",
    "dayjs": "^1.10.2",
    "esbuild": "^0.8.50",
    "glob": "^7.1.6",
    "halfmoon": "^1.1.1",
    "js-base64": "^3.6.0",
    "js-cookie": "^2.2.1",
    "notyf": "^3.9.0",
    "postcss": "^8.2.2",
    "postcss-import": "^14.0.0",
    "qs": "^6.9.6",
    "tailwindcss": "^2.0.2",
    "toastr": "^2.1.4"
  }
}
