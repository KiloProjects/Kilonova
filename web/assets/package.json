{
  "name": "KiloAssets",
  "version": "1.0.0",
  "description": "Assets for kilonova",
  "scripts": {
    "buildCSS": "TAILWIND_MODE=build postcss tailwind.css -o ../static/styles.css",
    "watchCSS": "TAILWIND_MODE=watch postcss tailwind.css -o ../static/styles.css --watch",
    "prodCSS": "NODE_ENV=production yarn buildCSS",
    "buildJS": "esbuild app.ts --bundle --outfile=../static/bundle.js --global-name=bundled --sourcemap",
    "watchJS": "esbuild app.ts --bundle --outfile=../static/bundle.js --global-name=bundled --sourcemap --watch",
    "prodJS": "esbuild app.ts --bundle --outfile=../static/bundle.js --global-name=bundled --minify --analyze",
    "prod": "yarn prodJS && yarn prodCSS",
    "build": "yarn buildJS && yarn buildCSS",
    "esv": "esbuild --version",
    "do_check": "tsc --noemit"
  },
  "license": "",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/js-cookie": "^3.0.2",
    "autoprefixer": "^10.4.12",
    "axios": "^0.27.2",
    "cssnano": "^5.1.13",
    "dayjs": "^1.11.5",
    "esbuild": "^0.15.9",
    "glob": "^8.0.3",
    "js-base64": "^3.7.2",
    "js-cookie": "^3.0.1",
    "notyf": "^3.10.0",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "preact": "^10.11.0",
    "preact-custom-element": "^4.2.1",
    "redux": "^4.2.0",
    "sprintf-js": "^1.1.2",
    "tailwindcss": "^3.1.8",
    "toastr": "^2.1.4",
    "underscore": "^1.13.6"
  }
}
