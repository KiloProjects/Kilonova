{
  "name": "Kilossets",
  "version": "1.0.0",
  "description": "Asset 'pipeline' for Kilonova",
  "scripts": {
    "buildCSS": "TAILWIND_MODE=build postcss tailwind.css -o ../static/styles.css",
    "prodCSS": "NODE_ENV=production yarn buildCSS",
    "buildJS": "esbuild app.js --bundle --target=es2015 --outfile=../static/bundle.js --global-name=bundled",
    "prodJS": "esbuild app.js --bundle --target=es2015 --minify --outfile=../static/bundle.js --global-name=bundled",
    "prod": "yarn prodJS && yarn prodCSS",
    "build": "yarn buildJS && yarn buildCSS",
    "esv": "esbuild --version"
  },
  "license": "",
  "private": true,
  "dependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@tailwindcss/forms": "^0.3.3",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.6",
    "dayjs": "^1.10.6",
    "esbuild": "^0.12.15",
    "glob": "^7.1.7",
    "js-base64": "^3.6.1",
    "js-cookie": "^2.2.1",
    "notyf": "^3.10.0",
    "postcss": "^8.3.5",
    "postcss-import": "^14.0.2",
    "query-string": "^7.0.1",
    "tailwindcss": "^2.2.4",
    "toastr": "^2.1.4",
    "underscore": "^1.13.1"
  }
}
